<Application x:Class="KhTracker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KhTracker"
             StartupUri="MainWindow.xaml"
             Exit="App_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="HVD_Comic_Serif_Pro">pack://application:,,,/Fonts/#HVD Comic Serif Pro</FontFamily>
            <FontFamily x:Key="KH2_ALL_MENU_I">pack://application:,,,/Fonts/#KH2 ALL MENU</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary1.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
